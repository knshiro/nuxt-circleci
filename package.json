{
  "name": "shippio-site",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "shinyatto <technac@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "npm run copy_env_development && HOST=0.0.0.0 PORT=3333 nuxt",
    "build": "npm run copy_env_development && nuxt build",
    "start": "PORT=3334 nuxt start",
    "generate": "nuxt generate",
    "generate:development": "npm run copy_env_development && NODE_ENV=development nuxt generate && cp static/d_robots.txt dist/robots.txt",
    "generate:staging": "npm run copy_env_staging && NODE_ENV=staging nuxt generate && cp static/d_robots.txt dist/robots.txt",
    "generate:production": "npm run copy_env_production && NODE_ENV=production nuxt generate",
    "generating_start:development": "npm run generate:development && PORT=3334 nuxt start",
    "deploy:demo": "npm run generate:staging && firebase deploy -P demo",
    "deploy:production": "npm run generate:production && firebase deploy -P production",
    "clear:demo": "rm -rf dist/* && touch dist/index.html && firebase deploy -P demo",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "lint-staged",
    "copy_env_development": "cp environment/.env.development .env",
    "copy_env_staging": "cp environment/.env.staging .env",
    "copy_env_production": "cp environment/.env.production .env",
    "remove_files_for_ghost_dist": "cd dist && rm d_robots.txt README.md .nojekyll sitemap.xml sitemap.xml.gz",
    "clean_theme": "cd shippio-blog-theme && rm -rf _nuxt",
    "generate:blog_theme": "npm run generate:production && npm run clean_theme && npm run remove_files_for_ghost_dist && cp -r dist/ shippio-blog-theme && cd shippio-blog-theme && npm run zip"
  },
  "config": {},
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 version",
    "IE 11"
  ],
  "dependencies": {
    "cross-env": "^5.2.0",
    "nuxt": "^2.4.0",
    "@nuxtjs/apollo": "^4.0.0-rc2",
    "@nuxtjs/babel-preset-app": "^0.8.0",
    "@nuxtjs/dotenv": "^1.1.1",
    "@nuxtjs/font-awesome": "^1.0.3",
    "@nuxtjs/google-tag-manager": "^2.1.0",
    "@nuxtjs/sitemap": "^0.1.1",
    "@nuxtjs/style-resources": "^0.1.2",
    "apollo-link-http": "^1.5.4",
    "babel-plugin-minify-dead-code-elimination": "^0.5.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue-app": "^2.0.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "bootstrap": "^4.1.3",
    "@nuxtjs/pwa": "^2.6.0",
    "cookie-universal-nuxt": "^2.0.11",
    "core-js": "^2.6.5",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "nuxt-i18n": "^4.0.0",
    "nuxt-robots-module": "^1.2.0",
    "prettier": "^1.13.5",
    "qonfucius-nuxt-fontawesome": "0.0.2",
    "shippio-style-vue": "git+ssh://git@github.com/shippio/shippio-style-vue#0.0.5",
    "textarea-autosize": "^0.4.2",
    "ts-loader": "^5.3.3",
    "vee-validate": "^2.1.1",
    "vue-analytics": "^5.12.2",
    "vue-moment": "^4.0.0-0",
    "vue-select": "^2.4.0",
    "vue-style-loader": "^4.1.0",
    "vue-textarea-autosize": "^1.0.4",
    "vue-timers": "^1.10.0",
    "vue2-google-maps": "^0.10.6"
  },
  "devDependencies": {
    "nodemon": "^1.18.9",
    "@nuxtjs/eslint-config": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2"
  }
}
